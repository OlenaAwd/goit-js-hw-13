{{#each hits}}
<div class="photo-card">
   <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
   <div class="info">
      <p class="info-item likes">
         <b>Likes</b>
         {{likes}} 
      </p>
      <p class="info-item views">
         <b>Views</b>
         {{views}}
      </p>
      <p class="info-item comments">
         <b>Comments</b>
         {{comments}}
      </p>
      <p class="info-item downloads">
         <b>Downloads</b>
         {{downloads}}
      </p>
   </div>
</div>
{{/each}} 